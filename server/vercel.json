{
    "version": 2,
    "functions": {
        "src/module/user/**/*.js": {
            "maxDuration": 60,
            "memory": 1024
        },
        "src/module/post/**/*.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/api/user/(.*)",
            "dest": "/src/module/user/$1.js"
        },
        {
            "src": "/api/post/(.*)",
            "dest": "/src/module/post/$1.js"
        }
    ]
}